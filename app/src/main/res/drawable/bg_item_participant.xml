<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/ring">
        <shape android:shape="oval">
            <solid android:color="?attr/colorSurface" />
            <size
                android:width="48dp"
                android:height="48dp" />
        </shape>
    </item>
    <item
        android:id="@+id/base_background"
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">
        <shape
            android:shape="oval"
            android:tint="#FCD366" />
    </item>
</layer-list>